   <div ng-controller="AssessmentsController" ng-init="GetAssessment(2)">
       <div>
       <label>Search All: <input ng-model="search" class="textBoxStyle" ></label> 
       <label>
           Sort By: <select id="ops" ng-model="sort" class="textBoxStyle">
               <option>'client'</option>
               <option>'job-number'</option>
               <option>'job-name'</option>
               <option>'due'</option>
               <option>'status'</option>
                       <option>-'client'</option>
                       <option>-'job-number'</option>
                       <option>-'job-name'</option>
                       <option>-'due'</option>
                       <option>-'status'</option>
           </select>
       </label> 
           </div>
           <div class="row, text-left">
               <table class="table table-bordered">
                   <tr class="tableHeader">
                       <th>
                           Client
                       </th>
                       <th>Job Number</th>
                       <th>Job Name</th>
                       <th>Due</th>
                       <th>Status</th>
                   </tr>
                   <tr ng-repeat="(key,value) in results | orderBy:sort | filter:search | startFrom:currentPage*pageSize | limitTo:pageSize | filter:search:strict">
                       <td> {{value.client}}</td>
                       <td> {{value['job-number']}}</td>
                       <td> {{value['job-name']}}</td>
                       <td> {{value.due}}</td>
                       <td class="tableCell1"> {{value.status}}</td>
                   </tr>
               </table>
               <div class="col-sm-offset-4 col-sm-4 text-center">
               <button ng-disabled="currentPage == 0" ng-click="currentPage=currentPage-1">
                   Previous
               </button>
               {{currentPage+1}}/{{numberOfPages()}}
                   <button ng-disabled="currentPage == numberOfPages()-1" ng-click="currentPage=currentPage+1">
                       Next
                   </button>
                   </div>
           </div>
</div>
